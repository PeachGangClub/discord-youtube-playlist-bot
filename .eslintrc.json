{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": ["plugin:prettier/recommended", "eslint:recommended"],
  "plugins": ["prettier"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "prettier/prettier": "error",
    "comma-dangle": "off"
  }
}
